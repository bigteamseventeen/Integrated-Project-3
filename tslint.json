{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "quotemark": false
    },
    "rules": {
        "quotemark": false,
        "ordered-imports": false,
        "encoding": true,
        "comment-format": false,
        "indent": false,
        "only-arrow-functions": false,
        "trailing-comma": false,
        "object-literal-sort-keys": false,
    },
    "rulesDirectory": []
}